    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.spec.+(ts)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
